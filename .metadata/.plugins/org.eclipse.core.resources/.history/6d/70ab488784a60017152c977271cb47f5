import java.awt.List;
import java.util.ArrayList;

public class Wioska {
Punkt cord;
int type; //0 - cel, 1 - off, 2 - fejk off, 3 - fejk def
ArrayList<Wioska> linked = new ArrayList<Wioska>();

Wioska(Punkt cord, int type) {
	this.cord = cord;
	this.type = type;
}

Wioska(Punkt cord) {
	this.cord = cord;
}

Wioska(int x, int y) {
	cord = new Punkt(x, y);
}



}
